{
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/feedback/:id", 
      "dest": "/api/feedback/:id"
    },
    {
      "src": "/aboutus",
      "dest": "/aboutus.html"
    },
    {
      "src": "/landing",
      "dest": "/landing.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
