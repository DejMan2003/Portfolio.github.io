{
  "builds": [
    {
      "src": "/index.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/aboutus",
      "destination": "/aboutus.html"
    },
    {
      "source": "/landing",
      "destination": "/landing.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}