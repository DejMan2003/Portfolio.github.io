{
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/aboutus",
      "destination": "/aboutus.html"
    },
    {
      "source": "/landing",
      "destination": "/landing.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/styles.css",
      "destination": "/assets/css/main.css"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "regions": ["sfo1"],
  "cleanUrls": true,
  "trailingSlash": false
}